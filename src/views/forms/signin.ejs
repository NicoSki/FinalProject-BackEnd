<%- layout("layouts/main.ejs") %>

    <% if(errorMessage) {%>
        <div class="container text-center bg-danger">
            <p>
                <%= errorMessage %>
            </p>
        </div>
        <% } %>

            <div class="caja1">
                <img class="logo1"
                    src="data:image/png;base64,"
                    alt="quikImage1">
                <h1>SIGN IN:</h1>
                <form action="/user/signin" method="POST">

                    <label for="username">NAME*</label>
                    <input type="text" name="username" placeholder="Write your name" required>

                    <label for="username">SURNAME*</label>
                    <input type="text" name="surname" placeholder="Write your surname" required>

                    <label for="username">PHONE NUMBER*</label>
                    <input type="number" name="phone" placeholder="Write your phone" min="0" required>

                    <label for="username">E-MAIL*</label>
                    <input type="email" name="email" placeholder="Example@gmail.com" required>

                    <label for="username">ADDRESS*</label>
                    <input type="text" name="address" placeholder="Address for sending products" required>

                    <label for="password">PASSWORD</label>
                    <input type="password" name="password" placeholder="**********" required>

                    <label for="password">REPEAT PASSWORD</label>
                    <input type="password" name="ppassword" placeholder="**********" required>

                    <input type="submit" value="Be part of QuikSilver!">

                    <br>

                    <a href="/user/login">I already have my QuikSilver account!</a>
                </form>
            </div>